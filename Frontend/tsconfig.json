{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "Frontend/.expo/types/**/*.ts",
    "Frontend/expo-env.d.ts",
    "Frontend/babel.config.js",
    "Frontend/nativewind-env.d.ts"
  ]
}
